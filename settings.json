{
  "workbench.welcome.enabled": false,
  "diffEditor.renderSideBySide": false,
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		".build/**": true,
		"out*/**": true,
		"i18n/**": true,
		"extensions/**/out/**": true,
		"*.min.js": true
	},

	"[javascript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features",
		"editor.formatOnSave": true,
		"files.trimTrailingWhitespace": true,
	},
	"javascript.format.insertSpaceAfterConstructor": true,
	"javascript.format.insertSpaceBeforeFunctionParenthesis": true,
	"javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,

	"files.autoSave": "afterDelay",
	"liveServer.settings.CustomBrowser": "chrome",
	"kite.showWelcomeNotificationOnStartup": false,
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"powermode.enabled": true,
	"powermode.explosions.maxExplosions": 1,
	"git.fetchOnPull": false,
	"code-runner.clearPreviousOutput": true,
	"glassit.alpha": 235,

	"vim.easymotion": true,
	"vim.incsearch": true,
	"vim.useSystemClipboard": true,
	"vim.useCtrlKeys": true,
	"vim.hlsearch": true,
	"vim.highlightedyank.enable": true,
	"vim.highlightedyank.textColor": "cyan",
	"vim.highlightedyank.color": "rgba(250, 240, 170, 0.1)",
	"vim.smartRelativeLine": true,
	"vim.camelCaseMotion.enable": true,
	"vim.autoindent": true,
	"vim.insertModeKeyBindings": [
		{
			"before": ["j", "j"],
			"after": ["<Esc>"]
		},
		{
			"before": ["h", "h"],
			"after": ["<Esc>"]
		},
		{
			"before": ["k", "k"],
			"after": ["<Esc>"]
		}
	],
	"vim.normalModeKeyBindings": [
		{
			// Delete this line
			"before": ["<leader>", "d"],
			"after": ["d", "d"]
		},
		{
			// Delete until space
			"before": ["d", "<leader>", "d"],
			"after": ["d", "t", "<Space>"]
		},		
    {
			// Yank until space
			"before": ["y", "<leader>", "y"],
			"after": ["y", "t", "<Space>"]
		},
		{
			"before": ["<C-n>"],
			"commands": [":nohl"]
		},
		{
			// Change to the next editor
			"before": ["<tab>"],
			"commands": ["workbench.action.nextEditor"]
		},
		{
			"before": ["<leader>", "<tab>"],
			"commands": ["workbench.action.previousEditor"]
		},
		{
			// !J
			"before": ["¿"],
			"after": ["f", "<Space>", "a", "<CR>", "<Esc>"]
		},
	],
	"vim.visualModeKeyBindings": [
		{
			// Better indent
			"before": [">"],
			"after": [">", "g", "v"]
		},
		{
			// Better indent
			"before": ["<"],
			"after": ["<", "g", "v"]
		},
	],
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			// Split editor vertically
			"before": ["s", "v"],
			"commands": ["workbench.action.splitEditor"]
		},
		{
			// Split editor horizontally
			"before": ["s", "s"],
			"commands": ["workbench.action.splitEditorDown"]
		},
		{
			// Jump cursor to another buffer
			"before": ["<C-Space>"],
			"after": ["<C-w>", "w"]
		},
		{
			// Move cursor to the left buffer
			"before": ["s", "h"],
			"after": ["<C-w>", "w"]
		},
		{
			// Move cursor to the right buffer
			"before": ["s", "l"],
			"after": ["<C-w>", "l"]
		},
		{
			// Move cursor to the down buffer
			"before": ["s", "j"],
			"after": ["<C-w>", "j"]
		},
		{
			// Move cursor to the up buffer
			"before": ["s", "k"],
			"after": ["<C-w>", "k"]
		},
		{
			// Fast scrolling
			"before": ["<C-e>"],
			"after": ["9", "<C-e>"]
		},
		{
			// Fast scrolling
			"before": ["<C-y>"],
			"after": ["9", "<C-y>"]
		},
		{
			// Insert a new line up 
			"before": ["<leader>", "<leader>", "o"],
			"after": ["O"]
		},
		{
			// Save and quit the current editor
			"before": ["<leader>", "q"],
			"commands": [":wq!"]
		},
		{
			// Toggle file sideBar
			"before": ["<leader>", "e"],
			"commands": ["workbench.action.toggleSidebarVisibility"]
		},
		{
			// Toggle activityBar
			"before": ["<leader>", "2", "e"],
			"commands": ["workbench.action.toggleActivityBarVisibility"]
		},
		{
			// Reveal current file in fileExplorer
			"before": ["<leader>", "f"],
			"commands": ["revealInExplorer"]
		},
		{
			// Reveal current file in fileExplorer
			"before": ["s", "f"],
			"commands": ["revealInExplorer"]
		},
		{
			"before": ["<leader>", "r", "f"],
			"commands": ["fileutils.renameFile"]
		},
		{
			// Fast horizontal movement 
			"before": ["<leader>", "h"],
			"after": ["_"]
		},
		{
			// Fast horizontal movement 
			"before": ["<leader>", "l"],
			"after": ["$"]
		},
		{
			// Decrement number
			"before": ["-"],
			"after": ["<C-x>"]
		},
		{
			// Increment number
			"before": ["+"],
			"after": ["<C-a>"]
		},
		{
			// Exit fast like EL ZORRO
			"before": ["<leader>", "z"],
			"after": ["Z", "Z"]
		},
		{
			// Open last file
			"before": ["<leader>", "o"],
			"after": ["<C-o>"]
		},
		{
			// Delete until '
			"before": ["<leader>", "C"],
			"after": ["d", "t", "'"]
		},
		{
			// Delete until "
			"before": ["<leader>", "c"],
			"after": ["d", "t", "\""]
		},
		{
			// Delete until >
			"before": ["<leader>", "m"],
			"after": ["d", "t", ">"]
		},
		{
			// Delete until <
			"before": ["<leader>", "M"],
			"after": ["d", "t", "<"]
		},
		{
			// Delete until ;
			"before": ["<leader>", "p"],
			"after": ["d", "t", ";"]
		},
		{
			// Delete until )
			"before": ["<leader>", "9"],
			"after": ["d", "t", ")"]
		},
		{
			// Delete until (
			"before": ["<leader>", "8"],
			"after": ["d", "t", "("]
		},
		{
			// Delete until .
			"before": ["<leader>", "."],
			"after": ["d", "t", "."]
		},
		{
			// from Cursor delete until ,
			"before": ["<leader>", ","],
			"after": ["d", "t", ",", "s", "<Esc>"]
		},
		{
			// Quick semi
			"before": ["<leader>", "2", ","],
			"after": ["$","a",","],
		},
		{
			// from C delete until ]
			"before": ["<leader>", "2", "}"],
			"after": ["d", "t", "]"]
		},
		{
			// from C delete until }
			"before": ["<leader>", "}"],
			"after": ["d", "t", "}"]
		},
		{
			// from C delete until /
			"before": ["<leader>", "7"],
			"after": ["d", "t", "/"]
		},
		{
			// Fast C Crolling down
			"before": ["<leader>", "j"],
			"after": ["9", "j"]
		},
		{
			// Fast C Crolling up 
			"before": ["<leader>", "k"],
			"after": ["9", "k"]
		},
		{
			// Fast scrolling to end of the current document
			"before": ["<leader>", "g"],
			"after": ["G", "G"]
		},
		{
			// Insert `
			"before": ["<leader>", "b"],
			"after": ["i", "`"]
		},
		{
			// Fast delete entire current document
			"before": ["<leader>", "backspace"],
			"after": ["g", "g", "d", "G"]
		},
		{
			// Select entire current document
			"before": ["<leader>", "a"],
			"after": ["g", "g", "v", "G"]
		},
		{
			// Change internal word
			"before": ["<leader>", "n"],
			"after": ["c", "i", "w"]
		},
		{
			// Change internal t
			"before": ["<leader>", "N"],
			"after": ["c", "i", "t"]
		},
		{
			// Change internal B
			"before": ["<leader>", "B"],
			"after": ["c", "i", "B"]
		},
		{
			// Change internal b
			"before": ["<leader>", "2", "b"],
			"after": ["c", "i", "b"]
		},
		{
			// Fold recursively
			"before": ["<leader>", "'"],
			"commands": ["editor.foldRecursively"]
		},
		{
			// Peek definition
			"before": ["<leader>", "i"],
			"commands": ["editor.action.peekDefinition"]
		},
		{
			// Run this document
			"before": ["<leader>", "r"],
			"commands": ["workbench.action.debug.run"]
		},
		{
			// Save the current document
			"before": ["<leader>", "s"],
			"commands": ["workbench.action.files.save"]
		},
		{
			// Search file in current folder
			"before": ["<leader>", "S"],
			"commands": ["workbench.view.search"]
		},
		{
			// Open terminal
			"before": ["<leader>", "ñ"],
			"commands": ["workbench.action.terminal.focusPreviousPane"]
		},
		{
			// Hide down Pane
			"before": ["<leader>", "u"],
			"commands": [
				"workbench.action.output.toggleOutput",
				"workbench.action.output.toggleOutput"
			]
		},
		{
			// Toggle comment line
			"before": ["<leader>", "1"],
			"commands": ["editor.action.commentLine"]
		},
		{
			// Toggle inlineFold
			"before": ["<leader>", "t"],
			"commands": ["inlineFold.toggle"]
		},
		{
			// rpl
			"before": ["<leader>", "2", "f"],
			"after": ["<Cmd>", ":", "%", "s", "/", "\"", "/", "'", "/", "g", "return"]
		},
		{
			// !rpl
			"before": ["<leader>", "3", "f"],
			"after": ["<Cmd>", ":", "%", "s", "/", "'", "/", "\"", "/", "g", "return"]
		}, 
		{
			// ! 2 -> 0
			"before": ["<leader>", "2", "0"],
			"after": ["<Cmd>", ":", "%", "s", "/",  ";", "/", "/", "g", "return"]
		},
	],
	"vim.leader": "<space>",
	"vim.handleKeys": {
		"<C-a>": false,
		"<C-f>": false,
		"<C-i>": true,
		"<C-w>": true,
		"<C-k>": true,
		"<M-k>": true,
	},

	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},

	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features",
		"editor.formatOnSave": true,
		"editor.codeActionsOnSave": {
			// For ESLint
			"source.fixAll.eslint": true,
			// For TSLint
			"source.fixAll.tslint": true,
			// For Stylelint
			"source.fixAll.stylelint": true,
			"editor.formatOnType": true
		}	
	},
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true,
		"editor.codeActionsOnSave": {
			// For ESLint
			"source.fixAll.eslint": true,
			// For TSLint
			"source.fixAll.tslint": true,
			// For Stylelint
			"source.fixAll.stylelint": true,
			"editor.formatOnType": true
		}	
	},

	"prettier.tabWidth": 2,
	"prettier.trailingComma": "none",
	"prettier.useTabs": true,

	"markdown-preview-enhanced.codeBlockTheme": "darcula.css",
	"markdown-preview-enhanced.previewTheme": "medium.css",
	"markdown-preview-enhanced.revealjsTheme": "night.css",

	"oneDarkPro.bold": true,
	"liveServer.settings.donotShowInfoMsg": true,
	"codesnap.transparentBackground": true,

	"window.title": "${folderName}",
	"terminal.integrated.fontSize": 18,

	"editor.lineDecorationsWidth": 22, // Adds padding to the left
	// "editor.rulers": [60], // Adds a line at 80 characters
	"workbench.editor.pinnedTabSizing": "compact",

	"[typescript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features",
		"editor.formatOnSave": true,
		"editor.codeActionsOnSave": {
			// For ESLint
			"source.fixAll.eslint": true,
			// For TSLint
			"source.fixAll.tslint": true,
			// For Stylelint
			"source.fixAll.stylelint": true,
			// For Stylelint
			"typescript.format.insertSpaceBeforeFunctionParenthesis": true,
			"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
			"typescript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": true,
			"editor.formatOnType": true,
			"files.trimTrailingWhitespace": true,
		}
	},
  "typescript.suggest.autoImports" : true,
	"typescript.format.insertSpaceBeforeFunctionParenthesis": true,
	"typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
	"typescript.format.insertSpaceAfterConstructor": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": true,

	// "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
	// Defines space handling after opening and before closing non empty brackets.
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	// Defines space handling after opening and before closing template string braces.
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,

	"[typescriptreact]": {
		"editor.defaultFormatter": "vscode.typescript-language-features",
		"editor.formatOnSave": true,
		"editor.codeActionsOnSave": {
			// For ESLint
			"source.fixAll.eslint": true,
			// For TSLint
			"source.fixAll.tslint": true,
			// For Stylelint
			"typescript.format.insertSpaceBeforeFunctionParenthesis": true,
			"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
			"typescript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": true,
			"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
			// Defines space handling after opening and before closing non empty brackets.
			"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
			// Defines space handling after opening and before closing template string braces.
			"typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
			"source.fixAll.stylelint": true,
			"editor.formatOnType": true
		}
	},

  "workbench.editor.labelFormat" : "short",

	"vitest.enable": true,
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[python]": {
		"editor.defaultFormatter": "ms-python.python"
