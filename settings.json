{
  "workbench.welcome.enabled": false,
  "diffEditor.renderSideBySide": false,
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		".build/**": true,
		"out*/**": true,
		"i18n/**": true,
		"extensions/**/out/**": true,
		"*.min.js": true
	},

	"[javascript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features",
		"editor.formatOnSave": true,
		"files.trimTrailingWhitespace": true,
	},
	"javascript.format.insertSpaceAfterConstructor": true,
	"javascript.format.insertSpaceBeforeFunctionParenthesis": true,
	"javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,

	"files.autoSave": "afterDelay",
	"liveServer.settings.CustomBrowser": "chrome",
	"kite.showWelcomeNotificationOnStartup": false,
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"powermode.enabled": true,
	"powermode.explosions.maxExplosions": 1,
	"git.fetchOnPull": false,
	"code-runner.clearPreviousOutput": true,
	"glassit.alpha": 255,

	"vim.easymotion": true,
	"vim.incsearch": true,
	"vim.useSystemClipboard": true,
	"vim.useCtrlKeys": true,
	"vim.hlsearch": true,
	"vim.highlightedyank.enable": true,
	"vim.highlightedyank.textColor": "cyan",
	"vim.highlightedyank.color": "rgba(250, 240, 170, 0.1)",
	"vim.smartRelativeLine": true,
	"vim.camelCaseMotion.enable": true,
	"vim.autoindent": true,
	"vim.insertModeKeyBindings": [
		{
			"before": ["j", "j"],
			"after": ["<Esc>"]
		},
		{
			"before": ["h", "h"],
			"after": ["<Esc>"]
		},
		{
			"before": ["k", "k"],
			"after": ["<Esc>"]
		}
	],
	"vim.normalModeKeyBindings": [
		{
			"before": ["<leader>", "d"],
			"after": ["d", "d"]
		},
		{
			"before": ["<C-n>"],
			"commands": [":nohl"]
		},
		{
			"before": ["<tab>"],
			"commands": ["workbench.action.nextEditor"]
		},
		{
			"before": ["<leader>", "<tab>"],
			"commands": ["workbench.action.previousEditor"]
		}
	],
	"vim.visualModeKeyBindings": [
		{
			"before": [">"],
			"after": [">", "g", "v"]
		},
		{
			"before": ["<"],
			"after": ["<", "g", "v"]
		}
	],
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": ["<leader>", "w"],
			"commands": ["workbench.action.splitEditor"]
		},
		{
			"before": ["<leader>", "e"],
			"commands": ["workbench.action.toggleSidebarVisibility"]
		},
		{
			"before": ["<leader>", "f"],
			"commands": ["revealInExplorer"]
		},
		{
			"before": ["<leader>", "r", "f"],
			"commands": ["fileutils.renameFile"]
		},
		{
			"before": ["<leader>", "h"],
			"after": ["_"]
		},
		{
			"before": ["<leader>", "l"],
			"after": ["$"]
		},
		{
			"before": ["<leader>", "z"],
			"after": ["Z", "Z"]
		},
		{
			"before": ["<leader>", "o"],
			"after": ["<C-o>"]
		},
		{
			"before": ["<leader>", "C"],
			"after": ["d", "t", "'"]
		},
		{
			"before": ["<leader>", "c"],
			"after": ["d", "t", "\""]
		},
		{
			"before": ["<leader>", "m"],
			"after": ["d", "t", ">"]
		},
		{
			"before": ["<leader>", "M"],
			"after": ["d", "t", "<"]
		},
		{
			"before": ["<leader>", "p"],
			"after": ["d", "t", ";"]
		},
		{
			"before": ["<leader>", "9"],
			"after": ["d", "t", ")"]
		},
		{
			"before": ["<leader>", "8"],
			"after": ["d", "t", "("]
		},
		{
			"before": ["<leader>", "."],
			"after": ["d", "t", "."]
		},
		{
			"before": ["<leader>", ","],
			"after": ["d", "t", ",", "s", "<Esc>"]
		},
		{
			"before": ["<leader>", "2", "}"],
			"after": ["d", "t", "]"]
		},
		{
			"before": ["<leader>", "}"],
			"after": ["d", "t", "}"]
		},
		{
			"before": ["<leader>", "7"],
			"after": ["d", "t", "/"]
		},
		{
			"before": ["<leader>", "j"],
			"after": ["9", "j"]
		},
		{
			"before": ["<leader>", "k"],
			"after": ["9", "k"]
		},
		{
			"before": ["<leader>", "g"],
			"after": ["G", "G"]
		},
		{
			"before": ["<leader>", "b"],
			"after": ["i", "`"]
		},
		{
			"before": ["<leader>", "backspace"],
			"after": ["g", "g", "d", "G"]
		},
		{
			"before": ["<leader>", "a"],
			"after": ["g", "g", "v", "G"]
		},
		{
			"before": ["<leader>", "n"],
			"after": ["c", "i", "w"]
		},
		{
			"before": ["<leader>", "N"],
			"after": ["c", "i", "t"]
		},
		{
			"before": ["<leader>", "B"],
			"after": ["c", "i", "B"]
		},
		{
			"before": ["<leader>", "2", "b"],
			"after": ["c", "i", "b"]
		},
		{
			"before": ["<leader>", "'"],
			"commands": ["editor.foldRecursively"]
		},
		{
			"before": ["<leader>", "i"],
			"commands": ["editor.action.peekDefinition"]
		},
		{
			"before": ["<leader>", "r"],
			"commands": ["workbench.action.debug.run"]
		},
		{
			"before": ["<leader>", "s"],
			"commands": ["workbench.action.files.save"]
		},
		{
			"before": ["<leader>", "S"],
			"commands": ["workbench.view.search"]
		},
		{
			"before": ["<leader>", "Ã±"],
			"commands": ["workbench.action.terminal.focusPreviousPane"]
		},
		{
			"before": ["<leader>", "u"],
			"commands": [
				"workbench.action.output.toggleOutput",
				"workbench.action.output.toggleOutput"
			]
		},
		{
			"before": ["<leader>", "1"],
			"commands": ["editor.action.commentLine"]
		}
	],
	"vim.leader": "<space>",
	"vim.handleKeys": {
		"<C-a>": false,
		"<C-f>": false,
		"<C-i>": true
	},

	"sync.gist": "ddcf965de3de1a942df4631b4f990687",

	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},

	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features",
		"editor.formatOnSave": true,
		"editor.codeActionsOnSave": {
			// For ESLint
			"source.fixAll.eslint": true,
			// For TSLint
			"source.fixAll.tslint": true,
			// For Stylelint
			"source.fixAll.stylelint": true,
			"editor.formatOnType": true
		}	
		
	},

	"prettier.tabWidth": 2,
	"prettier.trailingComma": "none",
	"prettier.useTabs": true,

	"markdown-preview-enhanced.codeBlockTheme": "darcula.css",
	"markdown-preview-enhanced.previewTheme": "medium.css",
	"markdown-preview-enhanced.revealjsTheme": "night.css",

	"oneDarkPro.bold": true,
	"liveServer.settings.donotShowInfoMsg": true,
	"codesnap.transparentBackground": true,

	"window.title": "${folderName}",
	"terminal.integrated.fontSize": 18,

	"editor.lineDecorationsWidth": 22, // Adds padding to the left
	// "editor.rulers": [60], // Adds a line at 80 characters
	"workbench.editor.pinnedTabSizing": "compact",

	"[typescript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features",
		"editor.formatOnSave": true,
		"editor.codeActionsOnSave": {
			// For ESLint
			"source.fixAll.eslint": true,
			// For TSLint
			"source.fixAll.tslint": true,
			// For Stylelint
			"source.fixAll.stylelint": true,
			"typescript.format.insertSpaceBeforeFunctionParenthesis": true,
			"editor.formatOnType": true,
			"files.trimTrailingWhitespace": true,
		}
	},
  "typescript.suggest.autoImports" : true,
	"typescript.format.insertSpaceBeforeFunctionParenthesis": true,
	"typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
	"typescript.format.insertSpaceAfterConstructor": true,
  "typescript.updateImportsOnFileMove.enabled": "always",

	"[typescriptreact]": {
		"editor.defaultFormatter": "vscode.typescript-language-features",
		"editor.formatOnSave": true,
		"editor.codeActionsOnSave": {
			// For ESLint
			"source.fixAll.eslint": true,
			// For TSLint
			"source.fixAll.tslint": true,
			// For Stylelint
			"typescript.format.insertSpaceBeforeFunctionParenthesis": true,
			"source.fixAll.stylelint": true,
			"editor.formatOnType": true
		}
	},

  "workbench.editor.labelFormat" : "short",

	"vitest.enable": true,
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[python]": {
		"editor.defaultFormatter": "ms-python.python"
	},
