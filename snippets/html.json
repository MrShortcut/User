	"<Img with attribs>": {
		"prefix": "img",
		"body": [
			"<img alt='${0:SEO}' loading='lazy' width={$1} height={$2} aria-label='${3:Descriptive image text}' /> ",
		],
		"description": "img <img attribs />"
	},

	"qtcom": {
		"prefix": "qtcom",
		"body": [
			"<!-- $2 :: @${1:qute comments for separating code} :: $2 -->",
			"<!-- $3                             |                            $3 -->",
		],
		"description": "qute comment for separating code",
	}

